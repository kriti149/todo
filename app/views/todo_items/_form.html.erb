<%= form_for([@todo_list, @todo_list.todo_items.build]) do |f| %>
  	<%= f.text_field :content, placeholder: "New Todo content" %>
  	<%= f.datetime_field :due_date, placeholder: "New Todo due date" %>
  	<%= f.number_field :priority, placeholder: "Should be between (1-5)" %>
  	<%= f.submit %>
<% end %>